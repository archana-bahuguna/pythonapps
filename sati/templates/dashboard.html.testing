                    {% else %}
                         <div><script> var userid={{userid}};</script></div>
                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="0" data-close-others="false" href="#" style="text-transform: lowercase">{{username}} <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu"><li><a href="/logout.html">logout</a></li></ul>
                        </li><!--//dropdown-->
                    {% endif %}
