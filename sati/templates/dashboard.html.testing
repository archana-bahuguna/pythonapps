                <!--ul class="nav nav-pills nav-justified text-center" role="tablist"-->
                    <li class="active"><a  class-"scroll" href="#feature-1" role="tab" data-toggle="tab"><i class="fa fa-cloud-upload"></i> <span class="hidden-sm hidden-xs">My Stats  &nbsp; &nbsp; </span></a></li>
                    <li><a  class="scroll" href="#feature-2" role="tab" data-toggle="tab"><i class="fa fa-tachometer"></i> <span class="hidden-sm hidden-xs">Journal something &nbsp; &nbsp; </span></a></li>
                    <li><a  class="scroll" href="#feature-3" role="tab" data-toggle="tab"><i class="fa fa-users"></i> <span class="hidden-sm hidden-xs">My thought cloud&nbsp; &nbsp; </span></a></li>
                    <li class="last"><a  class="scroll" href="#feature-4" role="tab" data-toggle="tab"><i class="fa fa-users"></i> <span class="hidden-sm hidden-xs">Wisdom of the world &nbsp; &nbsp; </span></a></li>
                </ul>
                <ul class="nav navbar-right">
                    {% if not loggedin %}
                        <li class="nav-item"><a href="/login.html">Log in</a></li>
